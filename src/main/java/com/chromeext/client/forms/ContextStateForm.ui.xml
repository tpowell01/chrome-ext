<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
             xmlns:ce="urn:import:com.chromeext.client.forms">
    <ce:BaseForm>
        <gwt:VerticalPanel width="100%">
            <gwt:HorizontalPanel width="100%">
                <gwt:Label>Token:</gwt:Label>
                <gwt:Label ui:field="lblToken"/>
            </gwt:HorizontalPanel>
            <gwt:HorizontalPanel width="100%">
                <gwt:Label>Number of pages:</gwt:Label>
                <gwt:Label ui:field="lblNumberOfPages"/>
            </gwt:HorizontalPanel>
            <gwt:HorizontalPanel width="100%">
                <gwt:Label>Elements count:</gwt:Label>
                <gwt:Label ui:field="lblElementsCount"/>
            </gwt:HorizontalPanel>
            <gwt:Label>Active predicates:</gwt:Label>
            <gwt:SimplePanel ui:field="spActivePredicates"/>
            <gwt:Label>Actions:</gwt:Label>
            <gwt:SimplePanel ui:field="spActions"/>
        </gwt:VerticalPanel>
    </ce:BaseForm>
</ui:UiBinder>