<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
             xmlns:ce="urn:import:com.chromeext.client.forms">
    <ce:BaseForm>
        <gwt:VerticalPanel width="100%">
            <gwt:Label>Contents</gwt:Label>
            <gwt:TextArea ui:field="taContents" enabled="false" width="290px" height="100px"/>
            <gwt:CheckBox ui:field="cbParseAddress">Parse Address</gwt:CheckBox>
            <gwt:HorizontalPanel width="100%">
                <gwt:cell horizontalAlignment="ALIGN_RIGHT">
                    <gwt:Button ui:field="btnSave">Save</gwt:Button>
                </gwt:cell>
            </gwt:HorizontalPanel>
        </gwt:VerticalPanel>
    </ce:BaseForm>
</ui:UiBinder>